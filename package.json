{
  "name": "justpromotion.ru",
  "version": "0.3.1",
  "description": "Personal blog of Blake Embrey",
  "private": true,
  "devDependencies": {
    "highlighter": "0.0.3",
    "metalsmith": "^2.1.0",
    "metalsmith-autoprefixer": "^1.0.2",
    "metalsmith-build-date": "^0.2.0",
    "metalsmith-clean-css": "^5.0.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-define": "^2.0.1",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-fingerprint": "^1.0.3",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-redirect": "^2.1.0",
    "metalsmith-templates": "^0.7.0",
    "moment": "^2.6.0",
    "onchange": "^2.5.0",
    "pug": "^2.0.0-beta4",
    "serve": "^1.4.0",
    "write-good": "^0.9.1"
  },
  "scripts": {
    "test": "write-good \"src/articles/**/*.md\"",
    "serve": "serve build -p ${PORT:-3000}",
    "watch": "onchange -i build.js \"{src,layouts}/**/*\" -- npm run build",
    "build": "node build.js",
    "deploy": "npm run build && cd build && git init . && git add . && git commit -m \"Deploy\"; git push \"git@github.com:ruslankonev/justpromotion.ru.git\" master:gh-pages --force && rm -rf .git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blakeembrey/blakeembrey.com.git"
  },
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "MIT"
}
